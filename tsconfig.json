{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "sourceMap": false,
    "strict": true,
    "lib": ["es6", "dom"],
    "jsx": "react"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
